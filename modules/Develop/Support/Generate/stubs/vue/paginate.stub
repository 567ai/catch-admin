<div class="pt-2 pb-2 flex justify-end">
    <el-pagination
      background
      layout="total,sizes,prev,pager,next"
      :current-page="query.page"
      :page-size="query.limit"
      @current-change="changePage"
      @size-change="changeLimit"
      :total="total"
      :page-sizes="[1, 10, 20, 30, 50]"
    />
</div>
